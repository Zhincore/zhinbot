{
  "name": "zhinbot",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "main": "dist/main",
  "type": "commonjs",
  "private": true,
  "license": "GPLV3 OR LATER",
  "scripts": {
    "prebuild": "prisma db push",
    "build": "swc src -d dist",
    "watch": "swc src -d dist -w -s inline",
    "start": "node .",
    "start:dev": "node --enable-source-maps .",
    "dev": "DRY_RUN=true nodemon --exec ts-node -r tsconfig-paths/register src/main.ts",
    "check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "generateDocs": "ts-node -r tsconfig-paths/register scripts/generateDocs/index.ts"
  },
  "pnpm": {
    "updateConfig": {
      "ignoreDependencies": [
        "chalk"
      ]
    }
  },
  "dependencies": {
    "@discordjs/voice": "^0.14.0",
    "@fluent/bundle": "^0.17.1",
    "@ltd/j-toml": "^1.37.0",
    "@prisma/client": "^4.8.0",
    "@swc/helpers": "^0.4.14",
    "bufferutil": "^4.0.7",
    "catch-exit": "^1.2.2",
    "chalk": "^4.0.0",
    "chrono-node": "^2.5.0",
    "color": "^4.2.3",
    "discord-api-types": "^0.37.25",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "emoji-regex": "^10.2.1",
    "erlpack": "github:discord/erlpack",
    "fastest-validator": "^1.16.0",
    "ms": "^2.1.3",
    "prisma": "^4.8.0",
    "reflect-metadata": "^0.1.13",
    "text-table": "^0.2.0",
    "typedi": "^0.10.0",
    "typescript": "^4.9.4",
    "utf-8-validate": "^5.0.10",
    "winston": "^3.8.2",
    "winston-journald3": "^0.1.2",
    "yaml": "^2.2.1",
    "youtube-dl-exec": "^2.1.10",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.24",
    "@types/color": "^3.0.3",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.11.18",
    "@types/text-table": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "chokidar": "^3.5.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.17.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1"
  }
}
