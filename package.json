{
  "name": "zhinbot",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "main": "dist/main",
  "type": "module",
  "private": true,
  "license": "GPLV3 OR LATER",
  "scripts": {
    "prebuild": "prisma db push",
    "build": "swc src -d dist",
    "build:dev": "swc src -d dist -s inline",
    "watch": "swc src -d dist -s inline -w",
    "start": "node .",
    "start:dev": "node --enable-source-maps .",
    "dev": "DRY_RUN=true nodemon --exec node --loader ./loader.js src/main.ts",
    "check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "generateDocs": "ts-node -r tsconfig-paths/register scripts/generateDocs/index.ts"
  },
  "pnpm": {
    "updateConfig": {
      "ignoreDependencies": [
        "chalk"
      ]
    }
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.16.0",
    "@fluent/bundle": "^0.18.0",
    "@ltd/j-toml": "^1.38.0",
    "@prisma/client": "^5.1.1",
    "@swc/helpers": "^0.5.1",
    "bufferutil": "^4.0.7",
    "chalk": "^4.0.0",
    "chrono-node": "^2.6.5",
    "color": "^4.2.3",
    "discord-api-types": "^0.37.54",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "emoji-regex": "^10.2.1",
    "erlpack": "github:discord/erlpack",
    "exit-hook": "^3.2.0",
    "fastest-validator": "^1.17.0",
    "js-levenshtein": "^1.1.6",
    "ms": "^2.1.3",
    "node-fetch": "^3.3.2",
    "prisma": "^5.1.1",
    "reflect-metadata": "^0.1.13",
    "sodium-native": "^4.0.4",
    "text-table": "^0.2.0",
    "typedi": "^0.10.0",
    "typescript": "^5.1.6",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.15.0",
    "utf-8-validate": "^6.0.3",
    "winston": "^3.10.0",
    "winston-journald3": "^0.1.2",
    "yaml": "^2.3.1",
    "yt-dlp-wrap": "^2.3.11",
    "zlib-sync": "^0.1.8"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.78",
    "@types/color": "^3.0.3",
    "@types/ms": "^0.7.31",
    "@types/node": "^20.5.1",
    "@types/text-table": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "chokidar": "^3.5.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-sonarjs": "^0.20.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  }
}
